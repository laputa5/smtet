#menu_header
  .menu_bar
  #menu_logo_container
    %img#menu_logo{:src => image_path('smtet/smtet_logo.jpg')}
  #menu_menu.clearfix
    - @menu_path_title_lookup.each do |path, title|
      - if params[:path] == path
        .menu_text.current_page= link_to title, path
      - else
        .menu_text= link_to title, path
  = render "pages/menu_images"
